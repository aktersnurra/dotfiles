# Language managers

# Init pyenv
eval "$(pyenv init --path)"
eval "$(pyenv virtualenv-init -)"

# Init fnm (node manager)
eval "$(fnm env)"

# Ocaml
[[ ! -r /home/aktersnurra/.opam/opam-init/init.zsh ]] || source /home/aktersnurra/.opam/opam-init/init.zsh  > /dev/null 2> /dev/null
eval "$(opam env)"
